				Button:
					id: countnormal
					size_hint: None, None
					size: '140dp', '40dp'
					background_normal: 'images/countnormal.png'
					background_down: 'images/countnormal.png'
					on_press: admindiagnosisrecord.countnormal()
					pos_hint: {"x": 0.12, "y": -0.90}
				Button:
					id: countmoderate
					size_hint: None, None
					size: '140dp', '40dp'
					background_normal: 'images/countmoderate.png'
					background_down: 'images/countmoderate.png'
					on_press: admindiagnosisrecord.countmoderate()
					pos_hint: {"x": 0.40, "y": -0.90}
				Button:
					id: countmoderate
					size_hint: None, None
					size: '140dp', '40dp'
					background_normal: 'images/countsevere.png'
					background_down: 'images/countsevere.png'
					on_press: admindiagnosisrecord.countsevere()
					pos_hint: {"x": 0.67, "y": -0.90}
			FloatLayout:
				ScrollableContainer3:
					id: scrollview3
					size: root.size
					pos_hint: {"x": -0.05, "y": 0.45}
					scroll_timeout: 55
					scroll_distance: 10
					app: app
					content_layout3:content_layout3
					GridLayout:
						id: content_layout3
						cols: 1
						size_hint: (1.0, None)
						pos: root.pos
						height: self.minimum_height
						padding: 25
						spacing: 28
