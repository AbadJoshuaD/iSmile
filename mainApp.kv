
#: import sm kivy.uix.screenmanager
ScreenManagement:
	transition: sm.FadeTransition()
	WelcomeScreen:
		name: 'welcome'
		Button:
			on_release: app.root.current = 'login'
			background_normal: 'images/welcomescreen.png'
			background_down: 'images/welcomescreen.png'
			font_size: 20
		Image:
			source: 'images/iconspin.zip'
			anim_delay: 1/10
			pos_hint:{'x': -0.2 ,'y': .19}
	LoginScreen:
		id: login
		txt_username: patientUser	
		txt_password: patientPass
		name: 'login'
		canvas.before:
			Rectangle:
				size: self.size
				source: "images/loginscreen.png"
		BoxLayout:
			orientation: 'vertical'
			spacing: 20
			padding: 50
			FloatLayout:
				TextInput:
					id: patientUser
					hint_text:'Patient Username'
					focus: False
					font_size:15
					size_hint: None, None
					pos_hint: {"x": 0.41, "y": .51}
					size: '170dp', '32dp'
					background_normal: 'images/white.png'
					background_active: 'images/white.png'
					input_filter: lambda text, from_undo: text[:15- len(self.text)]
					multiline: False
				TextInput:
		    		id: patientPass
				    hint_text:'Patient Password'
				    focus:False
				    password: True
				    font_size:15
				    size_hint: None, None
					pos_hint: {"x": 0.41, "y": .37}
					size: '170dp', '32dp'
					background_normal: 'images/white.png'
					background_active: 'images/white.png'
					input_filter: lambda text, from_undo: text[:15 - len(self.text)]
					multiline: False
				Button:
					id: btnLogin
					font_size:30
					background_normal: 'images/btnlogin.png'
					background_down: 'images/pbtnlogin.png'
					on_press: login.patient_login(patientUser.text,patientPass.text);patientinfoscreen.patient_info(patientUser.text);patientrecordscreen.displayInfo(patientUser.text)
					size_hint: None, None
					size: '150dp', '50dp'
					pos_hint: {"x": 0.29,"y":0.15}
				Button:
					id: btnSignup
					font_size:30
					background_normal: 'images/btnsignup.png'
					background_down: 'images/pbtnsignup.png'
					on_press: app.root.current = 'signup'
					size_hint: None, None
					size: '300dp', '50dp'
					pos_hint: {"x": 0.29,"y": 0.009}
				Button:
					id:btnAdminLogin
					background_normal: 'images/btnadminlogin.png'
					background_down: 'images/pbtnadminlogin.png'
					on_release: app.root.current = 'adminlogin'
					size_hint: None, None
					size: '150dp', '50dp'
					pos_hint: {"x": 0.505,"y":0.15}
	AdminLoginScreen:
		id: adminlogin
		name: 'adminlogin'
		txt_adminusername: adminUser	
		txt_adminpassword: adminPass
		canvas.before:
			Rectangle:
				size: self.size
				source: "images/adminloginscreen.png"
		BoxLayout:
			orientation: 'vertical'
			spacing: 20
			padding: 50
			FloatLayout:
				TextInput:
					id: adminUser
					hint_text:'Admin Username'
					focus: False
					font_size:15
					size_hint: None, None
					pos_hint: {"x": 0.41, "y": .51}
					size: '160dp', '32dp'
					background_normal: 'images/white.png'
					background_active: 'images/white.png'
					input_filter: lambda text, from_undo: text[:15- len(self.text)]
					multiline: False
				TextInput:
		    		id: adminPass
				    hint_text:'Admin Password'
				    focus:False
				    password: True
				    font_size:15
				    size_hint: None, None
					pos_hint: {"x": 0.41, "y": .37}
					size: '160dp', '32dp'
					background_normal: 'images/white.png'
					background_active: 'images/white.png'
					input_filter: lambda text, from_undo: text[:15 - len(self.text)]
				Button:
					id: btnAdminLogin
					font_size:30
					background_normal: 'images/abtnlogin.png'
					background_down: 'images/apbtnlogin.png'
					on_press: adminlogin.admin_login(adminUser.text,adminPass.text)
					size_hint: None, None
					size: '300dp', '50dp'
					pos_hint: {"x": 0.29,"y":0.15}
				Button:
					id: abtnBack
					font_size:30
					background_normal: 'images/backbtn.png'
					background_down: 'images/pbackbtn.png'
					on_press: app.root.current = 'login'
					size_hint: None, None
					size: '300dp', '50dp'
					pos_hint: {"x": 0.29,"y": 0.009}
	AdminMainScreen:
		id: adminmain
		name: 'adminmain'
		canvas.before:
			Rectangle:
				size: self.size
				source: "images/adminmainscreen.png"
		BoxLayout:
			orientation: 'vertical'
			spacing: 20
			padding: 50
			FloatLayout:
				Button:
					id: ambtnPatientInfo
					background_normal: 'images/MAIN/patient.png'
					background_down: 'images/MAIN/patient.png'
					size_hint: None, None
					size: '180dp', '120dp'
					pos_hint: {"x": 0.08,"y":0.70}
					on_press: app.root.current = 'adminpatientrecord';adminpatientrecord.displayRecordAdmin()
				Button:
					id: ambtnPatientRecord
					background_normal: 'images/MAIN/record.png'
					background_down: 'images/MAIN/record.png'
					size_hint: None, None
					size: '180dp', '120dp'
					pos_hint: {"x": 0.625,"y":0.70}
					on_press: app.root.current = 'admindiagnosisrecord';admindiagnosisrecord.displayDiagRecordAdmin()
				Button:
					id: ambtnGuidelines
					background_normal: 'images/MAIN/guidelines.png'
					background_down: 'images/MAIN/guidelines.png'
					size_hint: None, None
					size: '180dp', '120dp'
					pos_hint: {"x": 0.08,"y":0.1}
					on_press: app.root.current = 'adminguide'
				Button:
					id: ambtnLogout
					background_normal: 'images/MAIN/logout.png'
					background_down: 'images/MAIN/logout.png'
					size_hint: None, None
					size: '180dp', '120dp'
					pos_hint: {"x": 0.625,"y":0.1}
					on_press: app.root.current ='adminlogin';
	AdminPatientRecordScreen:
		id: adminpatientrecord
		name: 'adminpatientrecord'
		scrollview2: scrollview2
		canvas.before:
			Rectangle:
				size: self.size
				source: "images/adminpatientinforecord.png"
		Button:
			id: patientinfoback
			size_hint: None, None
			size: '100', '56dp'
			background_normal: 'images/back1.png'
			background_down: 'images/back2.png'
			pos_hint: {"x": 0, "top": 1.01}
			on_release: app.root.current = 'adminmain'
		FloatLayout:
			TextInput:
				id: searchpatientinforecord
				hint_text:'Search patient diagnosis record here'
				focus: False
				font_size:15
				size_hint: None, None
				pos_hint: {"x": 0.115, "y": 0.766}
				size: '500dp', '30dp'
				background_normal: 'images/white.png'
				background_active: 'images/white.png'
				input_filter: lambda text, from_undo: text[:15- len(self.text)]
				on_text:adminpatientrecord.searchPatientInfoRecord(self.text)
				multiline: False
		BoxLayout:
			orientation: 'vertical'
			FloatLayout:
				Label:	
					text: 'Username'
					font_size: 18
					color: 0, 0, 255, 1
					pos_hint: {"x": -0.37,"y": -0.0525}
				Label:	
					text: 'First Name'
					font_size: 18
					color: 0, 0, 255, 1
					pos_hint: {"x": -0.19,"y": -0.0525}
				Label:	
					text: 'Last Name'
					font_size: 18
					color: 0, 0, 255, 1
					pos_hint: {"x": 0.02,"y": -0.0525}
				Label:	
					text: 'Age'
					font_size: 18
					color: 0, 0, 255, 1
					pos_hint: {"x": 0.16,"y": -0.0525}
				Label:	
					text: 'Contact Number'
					font_size: 18
					color: 0, 0, 255, 1
					pos_hint: {"x": 0.34,"y": -0.0525}
			FloatLayout:
				ScrollableContainer2:
					id: scrollview2
					size: root.size
					pos_hint: {"x": -0.01, "y": 0.45}
					scroll_timeout: 75
					scroll_distance: 5
					app: app
					content_layout2:content_layout2
					GridLayout:
						id: content_layout2
						cols: 1
						size_hint: (1.0, None)
						pos: root.pos
						height: self.minimum_height
						padding: 25
						spacing: 28
	AdminRecordScreen:
		id: admindiagnosisrecord
		name:'admindiagnosisrecord'
