	SignupScreen:
		id: signup
		name: 'signup'
		txt_firstname:firstName
		txt_lastname:lastName
		txt_address:address
		txt_gender:gender
		txt_patientUsername:patientUsername
		txt_contact:patientcontact
		txt_patientage:patient_age
		txt_patientpass:patientpass
		txt_repatientpass:patientpass_retype
		txt_checkup:checkup
		canvas.before:
			Rectangle:
				size: self.size
				source: "images/signupscreen.png"
		BoxLayout:
			orientation: 'vertical'
			GridLayout
				cols:1
				rows:7
				row_default_height: 35
				row_force_default:True
				spacing: 20
				padding: 80, 100
				FloatLayout:
					TextInput:
						id: firstName
						color: 7, 9, 2, 0.2
						hint_text:'First Name'
						focus: False
						font_size:12
						background_normal: 'images/white.png'
						background_active: 'images/white.png'
						size_hint: None, None
						size: '250dp', '30dp'
						pos_hint: {"x": 0.025, "y": -0.65}	
						multiline: False
					TextInput:
						id: lastName
						color: 7, 9, 2, 0.2
						hint_text:'Last Name'
						focus:False
						font_size:12
						background_normal: 'images/white.png'
						background_active: 'images/white.png'
						size_hint: None, None
						size: '250dp', '30dp'
						pos_hint: {"x": 0.52, "y": -0.65}
						multiline: False
					TextInput:
		    			id: address
		    			color: 7, 9, 2, 0.2
		    			hint_text:'Address'
		    			focus:False	
		    			font_size:12
		    			size_hint: None, None
		    			background_normal: 'images/white.png'
						background_active: 'images/white.png'
		    			pos_hint: {"x": 0.025 , "y": -2}
		    			size: '430dp', '30dp'
		    			multiline: False
		    		Spinner:
		    			id: gender
		    			text:"Gender"
		    			values:["Male", "Female"]
		    			size: '75dp', '32dp'
		    			size_hint: None, None
		    			pos_hint: {"x": 0.85,"y":-2}
		    			background_color: (1, 0.105, 0.102, 1)
					TextInput:
		    			id: patientUsername
		    			color: 7, 9, 2, 0.2
		    			hint_text:'Username'
		    			focus:False
		    			font_size:15
		    			size_hint: None, None
		    			pos_hint: {"x": 0.025,"y":-3.3}
		    			size: '280dp', '32dp'
		    			background_normal: 'images/white.png'
						background_active: 'images/white.png'
						multiline: False
					TextInput:
		    			id: patientcontact
		    			color: 7, 9, 2, 0.2
		    			hint_text:'Contact Number'
		    			focus:False
		    			font_size:15
		    			pos_hint: {"x": 0.52,"y":-3.3}
		    			size_hint: None, None
		    			size: '280dp', '32dp'
		    			background_normal: 'images/white.png'
						background_active: 'images/white.png'
						multiline: False
						input_filter:'int'
					TextInput:
		    			id:patient_age
		    			size_hint: None, None
		    			size: '35dp', '32dp'
		    			hint_text: 'Age'
		    			focus: False
		    			font_size:12
		    			pos_hint: {"x": 0.7525,"y":-2}
		    			background_normal: 'images/white.png'
						background_active: 'images/white.png'
						multiline: False
						input_filter:'int'
					TextInput:
						id: patientpass
						color: 7, 9, 2, 0.2
						hint_text:'Password'
						focus:False
						font_size:15
						pos_hint: {"x": 0.025,"y":-4.65}
						password: True
						size_hint: None, None
						size: '280dp', '32dp'
						background_normal: 'images/white.png'
						background_active: 'images/white.png'
						multiline: False
					TextInput:
		    			id: patientpass_retype
		    			color: 7, 9, 2, 0.2
		    			hint_text:'Re-type Password'
		    			focus:False
		    			font_size:15
		    			pos_hint: {"x": 0.52,"y":-4.65}
		    			password: True
		    			size_hint: None, None
		    			size: '280dp', '32dp'
		    			background_normal: 'images/white.png'
						background_active: 'images/white.png'
						multiline: False
					Spinner: 
		    			id: checkup
		    			text:"Choose here..."
		    			values:["Last week", "Last month", "Last year", "Never have been in to a dentist"]
		    			pos_hint: {"x": 0.58,"y":-5.7}
		    			background_color: (1, 0.105, 0.102, 1)
		    			size: '230dp', '32dp'
		    			size_hint: None, None
		    			sync_height: True
		    			height: 25

		    		Button:
						id: btnCreateAcc
						font_size:12
						size: 0.5, 0.5
						size_hint: None, None
		    			size: '200dp', '40dp'
		    			background_normal: 'images/btnsignup.png'
		    			background_down: 'images/pbtnsignup.png'
						pos_hint: {"x": 0.67,"y":-8.75}
						on_press: signup.patient_signup(firstName.text,lastName.text,address.text,patient_age.text,gender.text,patientUsername.text,patientcontact.text,patientpass.text,patientpass_retype.text,checkup.text)
					Button:
						id: bckLogin
						font_size:12
						size_hint: None, None
		    			size: '200dp', '40dp'
						pos_hint: {"x": 0.35,"y":-8.75}
						background_normal: 'images/backbtn.png'
		    			background_down: 'images/pbackbtn.png'
		    			on_press: app.root.current = 'login'
